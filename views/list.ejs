<%- include("header")-%>
    <div class="box" id="heading">
        <h1> <%=listType%> </h1>
    </div>

    <div class="box">
        
            <% for(var i=0;i<newItems.length;i++){ %>
                <div class="item">
                    <input type="checkbox">
                    <p>  <%=newItems[i]%> </li> </p>
                </div>
            <% } %>
            <form class="item" action="/" method="post">
                <input type="text" name="item" placeholder="New Items" autocomplete="off">
                <button type="submit" name="list" value= <%=listType%> >+</button>
               </form>           
    </div>
    <%- include("footer")-%>
